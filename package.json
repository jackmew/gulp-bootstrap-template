{
  "name": "gulp-bootstrap-template",
  "version": "0.0.1",
  "description": "Template for jquery + bootstarp + gulp",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jackmew/gulp-bootstrap-template.git"
  },
  "author": "Jackmew",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jackmew/gulp-bootstrap-template/issues"
  },
  "homepage": "https://github.com/jackmew/gulp-bootstrap-template#readme",
  "browser": {
    "bootstrap": "./node_modules/bootstrap/dist/js/bootstrap.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "bootstrap": {
      "depends": [
        "jquery:jQuery"
      ]
    }
  },
  "dependencies": {
    "bootstrap": "^3.3.5",
    "jquery": "^2.1.4"
  },
  "devDependencies": {
    "browserify": "^11.0.1",
    "browserify-shim": "^3.8.10",
    "gulp": "^3.9.0",
    "vinyl-source-stream": "^1.1.0"
  }
}
